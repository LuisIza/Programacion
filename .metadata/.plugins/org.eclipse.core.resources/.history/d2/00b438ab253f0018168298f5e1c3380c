
public class Calculos {
	
	
	
	
	public static boolean esPrimo (int numero) {
		
		int contador= 2;
		boolean primo = true;
		while (primo && contador !=numero) {
			if (numero % contador ==0) 
				primo = false;
			contador ++;
					
		}
		return primo;
	}
	
	
	public int cuentaPrimos (int numero) {
		int total = 0;
		while (numero >1)
			esPrimo(numero);
		numero --;
		total++;
		return total;
	}

}
