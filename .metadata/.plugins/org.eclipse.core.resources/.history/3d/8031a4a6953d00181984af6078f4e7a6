
package eventos;
import java.awt.*;
import javax.swing.*;

public class Ventana extends JFrame {

	public Ventana () {
		super("Titulo");
		setSize(300,200);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setLayout (new FlowLayout (FlowLayout.LEFT));
		
		
		
		
		
		
		
		setVisible(true);
	}
	
	public static void main(String[] args) {
		Ventana ventana = new Ventana();

	}

}
